{
  "name": "Defund Wikipedia",
  "version": "1.0",
  "description": "Deletes donation notices of Wikipedia; which is clearly a biased platform",
  "permissions": ["activeTab", "https://en.wikipedia.org/*"],
  "content_scripts": [
    {
        "matches": ["https://en.wikipedia.org/*"],
        "js": ["/content_script.js"],
        "all_frames": true
    }
  ],
  "manifest_version": 2
}
